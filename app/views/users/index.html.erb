<%= will_paginate %>

<% @users.each do |user| %>

<div class="row">
  <div class="col-sm-6 col-md-4">

    <div class="caption">
      <% if user.image? %>
        <%= image_tag user.image.url %>
      <% else %>
        <%= profile_img(user) %>
      <% end %>
        <p>ID:<%= user.id %></p>
        <p>Name:<%= user.name %></p>
        <p>Profile:<%= user.profile %></p>
        <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
        <br />
    </div>
  </div>
</div>

<% end %>

<%= will_paginate %>
